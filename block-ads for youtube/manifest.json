{
    "name": "Youtube Delete Ads",
    "manifest_version": 2,
    "version": "0.0.1",
    "description": "Extension skip ads by khag",
    "content_scripts": [{
       "matches": [
          "https://www.youtube.com/*"
       ],
       "js": [
          "content.js"
       ]
    }],
    "browser_action": {
        "default_icon": "icon16.png"
      }
}
 
